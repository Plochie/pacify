    "generate-index": "find -type f -name \"*.tsx\" | xargs -i grep -EoH \"export [A-Za-z]+ [A-Za-z]+\" {} | awk '{gsub(/:/, \" \")} $1' | awk -F\" \" '{print $1}' | sed s/.tsx//g | sed s/ts//g | xargs -i echo \"export * from '{}';\""
